{
  "name": "@weser/style",
  "version": "1.0.0",
  "description": "Utils for using inline styles in React",
  "author": "Robin Weser <robin@weser.io>",
  "license": "MIT",
  "homepage": "https://github.com/robinweser/weserstack.git",
  "repository": "https://github.com/robinweser/weserstack.git",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/**"
  ],
  "browserslist": [
    "IE >= 11",
    "Firefox >= 60",
    "Safari >= 11.1",
    "Chrome >= 66",
    "ChromeAndroid >= 66",
    "iOS >= 11.3",
    "Edge >= 15"
  ],
  "scripts": {

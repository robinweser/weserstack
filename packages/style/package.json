  "keywords": [],
  "dependencies": {
    "@weser/array": "workspace:*",
    "@weser/keyframe": "workspace:*",
    "@weser/object": "workspace:*",
    "css-in-js-utils": "^3.1.0",
    "isobject": "^4.0.0",
    "rtl-css-js": "^1.16.1",
    "sort-css-media-queries": "^1.4.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "jsdom": "^25.0.1",
    "react": "^19.1.0",
    "rimraf": "^3.0.2",
    "styles-debugger": "^1.0.0",
    "typescript": "^5.9.2",
    "vitest": "^2.1.8"
  }
